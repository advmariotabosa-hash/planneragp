    "uuid": "^9.0.1",
    "@sendgrid/mail": "^8.1.0",
    "nodemailer": "^6.9.8",
    "googleapis": "^131.0.0",
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/express": "^4.17.21",
